{
  "name": "caseware-assignment",
  "version": "1.0.0",
  "description": "A simple app to show seat assignment of Caseware employees",
  "proxy": "http://localhost:3001",
  "engines": {
    "node": "v12.13.0", 
    "npm": "6.12.0"
  },
