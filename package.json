  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^5.0.1",
    "nodemon": "^2.0.1"
  }
}
